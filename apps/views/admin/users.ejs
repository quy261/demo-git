<!DOCTYPE html>
<html lang="en">
<head>
    
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> Admin Dashboard </title>
    <%- include ('layout/head') -%>
</head>
<body>
    <%- include ('layout/nav') -%>

    <!-- users table -->
    <div class="container">
        <h2>Users</h2>           
        <table class="table table-hover">
            <thead>
                <tr>
                <th>ID</th>
                <th>Email</th>
                <th>First_name</th>
                <th>Last_name</th>
                <th>Created_at</th>
                <th>Updated_at</th>
                </tr>
            </thead>
            <tbody>
                <% if(data && data.users){ %>
                        <% for(var i = data.users.length - 1; i >= 0; i--){ %>
                            <tr>
                                <td> <%= data.users[i].id %></td>
                                <td> <%= data.users[i].email %></td>
                                <td> <%= data.users[i].first_name %></td>
                                <td> <%= data.users[i].last_name %></td>
                                <td> <%= data.users[i].created_at %></td>
                                <td> <%= data.users[i].updated_at %></td>
                            </tr>
                        <% } %>                  
                <% } %>
            </tbody>
        </table>
    </div>

    <script text="text/javascript" src="/static/js/post.js"></script>  

</body>
</html>